{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "@": ["./src"],
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "src/**/*.d.ts",
        "src/**/*.ts",
        "src/**/*.js",
        "src/**/*.svelte",
        ".routify/**.js"
    ]
}
